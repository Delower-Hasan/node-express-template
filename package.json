{
  "name": "libertea-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=20.9.0 <21.0.0",
    "yarn": ">=1.22.0 <2.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": "yarn lint-prettier"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "create-module": "npx plop module",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|md)\"",
    "lint-prettier": "yarn lint:check && yarn prettier:check",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.667.0",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "mongoose-null-error": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.16",
    "psn-api": "^2.10.1",
    "redis": "^4.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
